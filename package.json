{
  "name": "cdp-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --open --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "start": "npm run build",
    "prebuild": "rimraf dist",
    "build": "npm-run-all -p build:*",
    "build:css": "npm-run-all sass postcss cssmin",
    "build:js": "npm-run-all babel compile",
    "build:assets": "cpy src/images dist",
    "build:html": "cpy src/index.html dist",
    "sass": "node-sass src/styles/main.scss -o dist",
    "postcss": "postcss dist/main.css -o dist/bundle.css -u autoprefixer",
    "cssmin": "cleancss dist/bundle.css -o dist/bundle.min.css -O2",
    "postcssmin": "rimraf dist/main.css",
    "babel": "babel src/scripts/main.js -d .tmp/",
    "compile": "browserify .tmp/main.js -o dist/bundle.js",
    "postcompile": "uglifyjs dist/bundle.js -o dist/bundle.min.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "browserify": "^16.3.0",
    "clean-css-cli": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cpy-cli": "^2.0.0",
    "css-loader": "^3.1.0",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "html-webpack-plugin": "^5.4.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.3.0",
    "uglify-js": "^3.6.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "browserslist": [
    "> 1%"
  ]
